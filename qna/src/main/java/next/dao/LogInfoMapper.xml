<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="LogInfo">
	<insert id="create">
		insert into LOGINFO values (#{writer}, #{message},#{createdDate})
	</insert>
	
	<select id="findAll" resultType="next.model.LogInfo">
		select * from LOGINFO
	</select>	
</mapper>